  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "requirejs": "^2.3.2",
    "tsconfig": "^5.0.3",
    "typescript": "^2.4.2",
    "webpack": "^1.13.2",
    "@types/cordova": "0.0.34",
    "whatwg-fetch": "^1.0.0"
  }
