{
    "name": "cordova-plugin-kumulos-sdk",
    "version": "4.2.4",
    "types": "./index.d.ts",
    "description": "Official Cordova SDK for integrating Kumulos services with your mobile apps",
    "cordova": {
        "id": "cordova-plugin-kumulos-sdk",
        "platforms": [
            "ios",
            "android"
        ]
    },
    "scripts": {
        "build": "tsc && webpack && rm src/core/*.js",
        "dist": "tsc && webpack ---mode production && rm src/core/*.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Kumulos/KumulosSdkCordova"
    },
    "keywords": [
        "ecosystem:cordova",
        "cordova-ios",
        "cordova-android",
        "kumulos",
        "kumulos sdk",
        "kumulos cordova"
    ],
    "author": "Shaoyun Li",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "@types/cordova": "0.0.34",
        "babel-core": "^6.17.0",
        "babel-loader": "^7.0.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-preset-env": "^1.7.0",
        "raven-js": "^3.25.1",
        "typescript": "^2.4.2",
        "webpack": "^4.8.3",
        "webpack-cli": "^3.3.12",
        "whatwg-fetch": "^1.0.0"
    }
}
